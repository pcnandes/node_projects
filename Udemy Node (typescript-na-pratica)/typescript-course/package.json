{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API Desenvolvida utilizando TS, NODE, ES6, MySQL e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "NODE_ENV=development node ./build/server/server.js",
    "unit-test": "NODE_ENV=test mocha node --opts build/test/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha node --opts build/test/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "TypeScript",
    "NODE",
    "ES6"
  ],
  "author": "Paulo",
