    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "lodash": "^4.17.19",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
